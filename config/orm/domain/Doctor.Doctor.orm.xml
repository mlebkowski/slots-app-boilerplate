<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Enraged\Domain\Doctor\Doctor" table="enraged_domain_doctor">
        <id name="id" type="ulid" column="id">
            <generator strategy="NONE"/>
        </id>
        <field name="external_id" type="integer"/>
        <field name="name" length="128"/>
        <field name="created_at" type="datetime_immutable"/>
        <field name="updated_at" type="datetime_immutable" nullable="true"/>
        <field name="deleted_at" type="datetime_immutable" nullable="true"/>
    </entity>
</doctrine-mapping>